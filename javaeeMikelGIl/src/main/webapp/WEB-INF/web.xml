<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>LoginServlet</servlet-name>
  	<display-name>LoginServlet</display-name>
  	<description></description>
  	<servlet-class>com.ipartek.formacion.javaee.servlets.LoginServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>RegistroServlet</servlet-name>
  	<display-name>RegistroServlet</display-name>
  	<description></description>
  	<servlet-class>com.ipartek.formacion.javaee.servlets.RegistroServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ConversorServlet</servlet-name>
  	<display-name>ConversorServlet</display-name>
  	<description></description>
  	<servlet-class>com.ipartek.formacion.javaee.servlets.ConversorServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>LoginBootstrapServlet</servlet-name>
  	<display-name>LoginBootstrapServlet</display-name>
  	<description></description>
  	<servlet-class>com.ipartek.formacion.javaee.libros.controladores.LoginServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>LoginServlet2</servlet-name>
  	<display-name>LoginServlet2</display-name>
  	<description></description>
  	<servlet-class>com.ipartek.formacion.javaee.libros.controladores.LoginServlet2</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>LoginServlet</servlet-name>
  	<url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>RegistroServlet</servlet-name>
  	<url-pattern>/registro</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ConversorServlet</servlet-name>
  	<url-pattern>/conversor</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LoginBootstrapServlet</servlet-name>
  	<url-pattern>/LoginBootstrapServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LoginServlet2</servlet-name>
  	<url-pattern>/LoginBootstrap/login</url-pattern>
  </servlet-mapping>
</web-app>


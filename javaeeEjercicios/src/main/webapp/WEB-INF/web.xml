<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>CargarProductos</servlet-name>
  	<display-name>CargarProductos</display-name>
  	<description></description>
  	<servlet-class>com.ipartek.javaeeEjercicios.servlets.CargarProductos</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>InsertarArticulosServlet</servlet-name>
  	<display-name>InsertarArticulosServlet</display-name>
  	<description></description>
  	<servlet-class>com.ipartek.javaeeEjercicios.controladores.InsertarArticulosServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>UsuariosServlet</servlet-name>
  	<display-name>UsuariosServlet</display-name>
  	<description></description>
  	<servlet-class>com.ipartek.javaeeEjercicios.controladores.UsuariosServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>CargarProductos</servlet-name>
  	<url-pattern>/CargarProductos</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>InsertarArticulosServlet</servlet-name>
  	<url-pattern>/InsertarArticulosServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>UsuariosServlet</servlet-name>
  	<url-pattern>/login</url-pattern>
  </servlet-mapping>
</web-app>

<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>HoraServlet</servlet-name>
  	<display-name>HoraServlet</display-name>
  	<description></description>
  	<servlet-class>com.ipartek.formacion.ajax.HoraServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>LibrosServlet</servlet-name>
  	<display-name>LibrosServlet</display-name>
  	<description></description>
  	<servlet-class>com.ipartek.formacion.ajax.LibrosServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>HoraServlet</servlet-name>
  	<url-pattern>/hora</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LibrosServlet</servlet-name>
  	<url-pattern>/libros</url-pattern>
  </servlet-mapping>
</web-app>

        http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="entrada" 
        class="com.ipartek.formacion.springbasico.EntradaConstante"/>
    
    <bean id="salida"
        class="com.ipartek.formacion.springbasico.SalidaConsolaBonita">
